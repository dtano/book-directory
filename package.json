{
  "name": "bookdirectory",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "env-cmd -f ./config/test.env jest --watchAll --verbose --detectOpenHandles",
    "dev": "env-cmd -f ./config/dev.env concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "client": "npm start --prefix client",
    "server": "nodemon server.js",
    "lint": "eslint --fix --ext .js,.jsx .",
    "migrate:dev": "env-cmd -f ./config/dev.env sequelize-cli db:migrate",
    "migrate:test": "env-cmd -f ./config/test.env sequelize-cli db:migrate",
    "seed:test": "env-cmd -f ./config/test.env sequelize-cli db:seed:all",
    "seed:test:undo": "env-cmd -f ./config/test.env sequelize-cli db:seed:undo:all",
    "migrate:dev:undo": "env-cmd -f ./config/dev.env sequelize-cli db:migrate:undo",
    "migrate:test:undo": "env-cmd -f ./config/test.env sequelize-cli db:migrate:undo"
  },
  "author": "Denzel Joaquin Tano",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.19.0",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "bcrypt": "^5.0.1",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.29.4"
  }
}
